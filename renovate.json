{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "matchDepTypes": [
        "engines"
      ],
      "enabled": false
    },
    {
      "groupName": "deps",
      "matchDepTypes": [
        "dependencies",
        "devDependencies"
      ]
    }
  ],
  "rangeStrategy": "bump"
}
